{
  "compilerOptions": {
    "target": "es2020", 
    "module": "commonjs", 
    "esModuleInterop": true, 
    "strict": true,
    "forceConsistentCasingInFileNames": true, 
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types":["node","jest"],
    "resolveJsonModule": true,
  },
  "include": ["src/**/*.ts", "test/**/*.ts"],
}